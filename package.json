  },
  "devDependencies": {
    "@stencil/core": "^2.17.1",
    "@types/jest": "^27.0.3",
    "jest": "^27.0.3",
    "jest-cli": "^27.5.1",
    "jest-config": "^27.5.1",
    "np": "^6.5.0",
    "prettier": "^2.1.2",
    "rollup": "^2.32.0",
    "typescript": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bigcartel/stencil-router.git"
